<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <!-- root -->
  <!-- Barême d'entraide des tracteurs en France -->
  <!-- Coût indicatif pour des durées d'utilisation entre 500 et 900 Heures / an -->
  <!-- Coût avec carburant -->
  <nomenclature name="running_costs" hierarchical="false" translateable="true">
    <property-natures>
      <property-nature name="origin"                     type="choice"  choices="french_cooperation_scale" required="true"/>
      <property-nature name="variant"                    type="choice"  nomenclature="product_nature_variants" required="true"/>
      <property-nature name="minimum_power"              type="integer" required="true" default="0"/><!-- puissance minimale => power_unit -->
      <property-nature name="maximum_power"              type="integer" required="true" default="720"/><!-- puissance maximale => power_unit-->
      <property-nature name="power_unit"            type="choice"  nomenclature="units" required="true"/>
      <property-nature name="average_purchase_price"     type="decimal" required="true" />
      <property-nature name="hourly_cost"                type="decimal" required="true" /><!--  -->
      <property-nature name="year"                       type="integer" required="true" /><!--  -->
      <property-nature name="currency"                   type="choice"  nomenclature="currencies" required="true" default="EUR"/><!-- EUR -->
    </property-natures>
    <items>
      <!-- Tableau Tracteur -->

      <item name="2014_small_tractor_56_to_64_fhp"
