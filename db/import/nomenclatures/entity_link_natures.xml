<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <nomenclature name="entity_link_natures" notions="entity, linked">
    <property-natures>
      <property-nature name="entity" type="symbol"/>
      <property-nature name="linked" type="symbol"/>
      <property-nature name="symmetric" type="boolean" default="false" required="true"/>
    </property-natures>
    <items>
      <item name="undefined"       entity="acquaintance" linked="acquaintance"  symmetric="true"/>
      <item name="direct_kinship"  entity="parent"       linked="child"/>
      <item name="sibling_kinship" entity="sibling"      linked="sibling" symmetric="true"/>
      <item name="collaboration"   entity="collaborator" linked="collaborator"  symmetric="true"/>
      <item name="transform"       entity="old_organization"   linked="new_organization"/>
      <item name="membership"      entity="member"       linked="organization"/>
      <item name="hierarchy"       entity="manager"      linked="managee"/>
    </items>
  </nomenclature>
</nomenclatures>
