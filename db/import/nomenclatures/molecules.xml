<?xml version="1.0" encoding="UTF-8"?>
<nomenclatures xmlns="http://www.ekylibre.org/XML/2013/nomenclatures">
  <nomenclature name="molecules">
    <property-natures>
      <!-- SMILES Notation: wikipedia.org/wiki/Simplified_Molecular_Input_Line_Entry_Specification -->
      <property-nature name="formula" type="string"/>
    </property-natures>
    <items>
      <item name="ammonia"                       formula="N(H)(H)(H)"/>
      <item name="ammonium"                      formula="N(H)(H)(H)H"/>
      <item name="dinitrogen"                    formula="N#N"/>
      <item name="dioxygen"                      formula="O=O"/>
      <item name="diphosphorus_pentoxide"        formula="(=O)P(=O)(=O)P(=O)(=O)"/>
      <item name="tetraphosphorus_decaoxide"     formula="O=P13OP2(=O)OP(=O)(O1)OP(=O)(O2)O3"/>
      <item name="potassium_ferrocyanide"        formula="C(#N)[Fe-4](C#N)(C#N)(C#N)(C#N)C#N.O.O.O.[K+].[K+].[K+].[K+]"/>
      <item name="phosphate"                     formula="[O-]P(=O)([O-])[O-]"/>
      <item name="potassium_oxide"               formula="[K]O[K]"/>
      <item name="sulfur_dioxide"                formula="O=S=O"/>
      <item name="sodium_chloride"               formula="[Na+].[Cl-]"/>
      <item name="copper_sulfate"                formula="[Cu+2].[O-]S(=O)(=O)[O-]"/>
      <item name="dicopper_sulfate"              formula="[O-]S(=O)(=O)[O-].[Cu+].[Cu+]"/>
      <item name="glucose"                       formula="OC[C@@H](O1)[C@@H](O)[C@H](O)[C@@H](O)[C@@H](O)1"/>
      <item name="sucrose"                       formula="C([C@@H]1[C@H]([C@@H]([C@H]([C@H](O1)O[C@]2([C@H]([C@@H]([C@H](O2)CO)O)O)CO)O)O)O)O"/>
      <item name="malic_acid"                    formula="O=C(O)CC(O)C(=O)O"/>
      <item name="lactic_acid"                   formula="CC(O)C(=O)O"/>
      <item name="potassium_bicarbonate"         formula="[K+].[O-]C(=O)O"/>
      <item name="calcium_carbonate"             formula="[Ca+2].[O-]C([O-])=O"/>
      <item name="water"                         formula="(H)O(H)"/>
      <item name="ethanol"                       formula="[CH3][CH2][OH]"/>
    </items>
  </nomenclature>
</nomenclatures>
